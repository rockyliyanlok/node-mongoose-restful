{
  "name": "mongoose-restful",
  "version": "1.0.1",
  "description": "A tool to convert request query to mongoose find parameters: filter, select and options etc.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-destructuring",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockyliyanlok/node-mongoose-restful.git"
  },
  "keywords": [
    "mongoose",
    "restful",
    "query",
    "parser"
  ],
  "author": "Rocky Li",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-http": "^4.2.1",
    "chai-string": "^1.5.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.2",
    "mocha": "^6.1.4",
    "qs": "^6.7.0"
  },
  "dependencies": {
    "@rockyli/timestamp": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/rockyliyanlok/node-mongoose-restful/issues"
  },
  "homepage": "https://github.com/rockyliyanlok/node-mongoose-restful#readme"
}
